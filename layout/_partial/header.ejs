<header>
  <div class="mdui-appbar">
  <div class="mdui-toolbar mdui-color-theme">
    <a href="javascript:;" class="mdui-btn mdui-btn-icon"><i class="mdui-icon material-icons">&#xe5d2;</i></a>
    <a href="javascript:;" class="mdui-typo-title"><%= config.title %></a>
    <div class="mdui-toolbar-spacer"></div>
    <a href="javascript:;"  class="mdui-btn mdui-btn-icon"><i class="mdui-icon material-icons">&#xe8b6;</i></a>
    <a href="javascript:;" class="mdui-btn mdui-btn-icon"><i class="mdui-icon material-icons">&#xe5d5;</i></a>
    <a href="javascript:;" class="mdui-btn mdui-btn-icon"><i class="mdui-icon material-icons">&#xe5d4;</i></a>
  </div>
  <!--
  <div class="mdui-tab mdui-color-theme" mdui-tab>
    <a href="#example3-tab1" class="mdui-ripple mdui-ripple-white">web</a>
    <a href="#example3-tab1" class="mdui-ripple mdui-ripple-white">shopping</a>
    <a href="#example3-tab1" class="mdui-ripple mdui-ripple-white">videos</a>
  </div>
  -->
  </div>

  <div class="mdui-appbar mdui-appbar-fixed">
        <div class="mdui-toolbar mdui-color-theme-800">
            <a class="mdui-btn mdui-btn-icon" mdui-drawer="{target: '#left-drawer'}">
                <i class="mdui-icon material-icons">&#xe5d2;</i>
            </a>
            <div class="mdui-toolbar-spacer"> </div>
            <span class="mdui-typo-headline"><%= config.title %></span>
            <div class="mdui-toolbar-spacer"> </div>
            <a class="mdui-btn mdui-btn-icon" mdui-menu="{target: '#share_menu'}">
                <i class="mdui-icon material-icons">share</i>
            </a>
            <ul class="mdui-menu" id="share_menu">
                <li class="mdui-menu-item">
                </li>
            </ul>
        </div>
        
    </div>
    <div class="mdui-drawer mdui-drawer-close mdui-drawer-scrollbar" id="left-drawer" style="overflow-y: hidden;">
        <div class="mdui-hidden-md-up">
        </div>
        <ul class="mdui-list">
        <% for (var i in theme.sidebar.pages){ %>
        <a class="mdui-list-item mdui-ripple" href="<%= theme.sidebar.pages[i].link %>">
          <% if(theme.sidebar.pages[i].icon){ %>
            <i class="mdui-list-item-icon mdui-icon material-icons"><%= theme.sidebar.pages[i].icon %></i>
          <% } %>
          <%= i %>
          <div class="mdui-list-item-content"><%= i %></div>
        </a>
        <% } %>
        <%= i %>
        </ul>
    </div>
</header>
